QCheckBox {
    spacing: 0px;
}

QCheckBox::indicator {
    width: 20px;
    height: 20px;
}

/* Unchecked State */
QCheckBox::indicator:unchecked {
    border: 2px solid #E0E4EC;
    background-color: white;
}

/* Checked State */
QCheckBox::indicator:checked {
    border: 2px solid #7892DF;
    background-color: #7892DF;
    image: url(:/icons/icons/tick.png)  /* Path to your check mark icon */
}

/* Hover State */
QCheckBox::indicator:hover {
    border: 2px solid rgba(194, 197, 204, 255);
}

/* Disabled State */
QCheckBox::indicator:disabled {
    border: 2px solid #E0E4EC;
    background-color: #F6F6F6;
}


