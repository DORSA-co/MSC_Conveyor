QSpinBox, QDoubleSpinBox  
{
	background-color: transparent;
	border-bottom: 2px solid #D7D7D9;
	border-radius: None;
	font-size: 18px;
	min-height: 25px;
	min-width: 70px;
	qproperty-alignment: AlignCenter;
}

QSpinBox:disabled ,
QDoubleSpinBox:disabled 
{
	border-bottom: 2px solid #F0F0F2;
	color: rgb(120, 120, 120);
}

QSpinBox::up-arrow, QDoubleSpinBox::up-arrow
{   
	image: url(:/general_icons/icons/general_icons/plus_icon_black.png);
	width: 16px;
	height: 16px;
}

QSpinBox::down-arrow ,  QDoubleSpinBox::down-arrow
{   
	image: url(:/general_icons/icons/general_icons/minus_icon_black.png);
	width: 16px;
	height: 16px;
}

QSpinBox::up-arrow:disabled, QDoubleSpinBox::up-arrow:disabled
{   
	image: url(:/general_icons/icons/general_icons/plus_icon_gray.png);
	width: 16px;
	height: 16px;
}

QSpinBox::down-arrow:disabled ,  QDoubleSpinBox::down-arrow:disabled
{   
	image: url(:/general_icons/icons/general_icons/minus_icon_gray.png);
	width: 16px;
	height: 16px;
}

QSpinBox::up-button
{
	border:none;
    min-width:30px;
    min-height: 29px;
    subcontrol-origin: margin;
    subcontrol-position: right;
    top: 0px;
    right: 0px;
}

QSpinBox::down-button
{
    min-width:30px;
    min-height: 29px;
    subcontrol-origin: margin;
    subcontrol-position: left;
    top: 0px;
    right: 0px;
}

QSpinBox::up-button,
QSpinBox::down-button,
QDoubleSpinBox::up-button,
QDoubleSpinBox::down-button   {
	background-color: transparent;
}

QSpinBox::up-button:disabled ,
QSpinBox::down-button:disabled ,
QDoubleSpinBox::up-button:disabled ,
QDoubleSpinBox::down-button:disabled    {
    subcontrol-origin: border;
}

QSpinBox:focus, QDoubleSpinBox:focus{
	border-bottom: 2px solid #7892DF;
}

/************************************************************/

QTabWidget {
    background-color: #F7F8FA;
}

/* Tab Bar Style */
QTabBar::tab {
    background-color: #BDBDBF;
    color: rgb(20, 20, 20);
	min-width: 100px;
    padding: 8px 16px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:hover {
    background-color: #D7D7D9;
	color: rgb(120, 120, 120);
}

/* Tab Content Area */
QTabWidget::pane {
    border: 1px solid #E0E4EC;
    border-top: none;
    background-color: #E0E4EC;
}

/* Selected Tab Label Style */
QTabBar::tab:selected {
    background-color: #E0E4EC;
    border-bottom: 2px solid #7892DF; /* Change the color of the selected tab indicator */
}

/************************************************************/

QTableWidget {
    background-color: #F7F8FA;
	gridline-color: #D7D7D9;
	color: rgb(20, 20, 20);
	border: 2px solid #F7F8FA;
	border-radius: 8px;
}

QHeaderView::section {
    background-color: #F7F8FA;
    padding: 5px;
	border-top-left-radius: 0px;
	border-bottom: 2px solid #BDBDBF;
	border-right: 1px solid #D7D7D9;
	font-weight: bold;
	color: rgb(20, 20, 20);
}

QHeaderView::section:first {
   border-top-left-radius: 4px;
	border-left: None;
}

QHeaderView::section:last {
   border-top-right-radius: 4px;
	border-right: None;
}

QTableWidget::item:selected {
    background-color: #E0E4EC;
	color: rgb(20, 20, 20);
}

QTableWidget::item:hover {
    background-color: #D7D7D9;
}

QHeaderView::up-arrow {
	
	image: url(:/general_icons/icons/general_icons/table_sort_icon.png);
}

QScrollBar:horizontal, QScrollBar:vertical {
    border: 1px solid #BDBDBF;
    background: #BDBDBF;
    height: 4px;
}

QScrollBar::handle:horizontal, QScrollBar::handle:vertical {
    background: #646466;
}


